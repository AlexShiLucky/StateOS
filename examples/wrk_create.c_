#include <stm32f4_discovery.h>
#include <os.h>

sem_t *sem;

void slave()
{
	sem_wait(sem);
	LED_Tick();
}

void master()
{
	tsk_delay(1000*MSEC);
	sem_give(sem);
}

int main()
{
	LED_Init();

	sem = sem_create(0, semCounting);
	wrk_create(0, slave,  256);
	wrk_create(0, master, 256);
	tsk_stop();
}
