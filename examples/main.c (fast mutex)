#include <stm32f4_discovery.h>
#include <os.h>

static_MUT(mut);
static_TSK(sla, 0, slave);
static_TSK(mas, 0, master);

void slave()
{
	mut_wait(mut);
	LED_Tick();
	mut_give(mut);
}

void master()
{
	mut_wait(mut);
	tsk_delay(SEC);
	mut_give(mut);
}

int main()
{
	LED_Config();

	tsk_start(sla);
	tsk_start(mas);
	tsk_stop();
}
